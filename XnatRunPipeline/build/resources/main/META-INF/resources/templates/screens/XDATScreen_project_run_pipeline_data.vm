				if(data != null){


					// To sort data
					const sortObject = obj => Object.keys(obj).sort().reduce((res, key) => (res[key] = obj[key], res), {});

					data = sortObject(data);
					//data = data.sort()
					console.log(data)
					var $select = $("#subsession_list");                           // Locate HTML DOM element with ID "someselect".
