$(function (){

	let _url = "/xapi/int-run-pipline/get-sessions/$project";
	let selectedSubject = "";

	$('#subjects_list').change(function() {

	selectedSubject = $('option:selected', $(this)).text();

	console.log(selectedSubject)

	console.log(_url);

	
		$.ajax({
			url: _url,
			method:'POST',
